<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :notes %>
  <%= f.text_field :notes %>
  <br />

  <%= f.submit %>
<% end %>


